{
    "title": "xfoss.com home/donation/index 页面",
    "margin": {
        "top": 79,
        "right": 62,
        "bottom": 62,
        "left": 74
    },
    "headerTemplate": null,
    "footerTemplate": null,
    "language": "zh-hans",
    "github": {
        "url": "https://github.com/gnu4cn/buy-me-a-coffee"
    },
    "plugins": [
        "include-codeblock@^3.0.2",
        "tbfed-pagefooter@^0.0.1",
        "sectionx@^3.1.0",
        "sitemap",
        "code",
        "-lunr",
        "-search",
        "-sharing",
        "github-edit",
        "splitter",
        "theme-door",
        "search-pro-fixed"
    ],
    "variables": {
        "doorTheme": {
            "nav": [
                {
                    "url": "https://java.xfoss.com",
                    "target": "_blank",
                    "name": "学习 Java ☕"
                }
            ]
        }
    },
    "pluginsConfig": {
        "doorTheme": {
            "search-placeholder": "请输入关键字搜索",
            "logo": "./docs/images/rust-social-wide.jpg",
            "searchIcon": "./docs/images/search-icon.png",
            "favicon": "./docs/images/rust-fav-icon.png"
        },
        "github-edit": {
            "repo": "gnu4cn/rust-lang",
            "branch": "main"
        },
        "include-codeblock": {
            "template": "ace",
            "unindent": true,
            "edit": true
        },
        "tbfed-pagefooter": {
            "copyright": "Copyleft @ rust.xfoss.com 2022",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "sectionx": {
            "tag": "b"
        },
        "sitemap": {
            "hostname": "https://rust-lang.xfoss.com/"
        }
    }
}
